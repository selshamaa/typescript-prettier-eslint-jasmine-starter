{
  "name": "typescript-prettier-eslint-jasmine-starter",
  "version": "1.0.0",
  "description": "starter project using typescript, prettier, eslint and jasmine",
  "main": "index.js",
  "scripts": {
    "jasmine": "jasmine",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "build": "npx tsc",
    "test": "npm run build && npm run jasmine"
  },
  "author": "Sherif Elshamaa",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
